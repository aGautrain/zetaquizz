/** Delete all rooms (including the lobby) */
macroscript clearRooms () {
	
	var { result: {list: rooms } } = sudo zpUtils::GLOBAL_OWNER call zpGroupRecipe::getGroupList({}) hardFail;
	
	for room in rooms {
		sudo zpUtils::GLOBAL_OWNER call zpGroupRecipe::deleteGroup({id: room.id});
	}

} return { rooms } 